#!groovy
/*
=========================================
Author: Devops
Date: 11-12-2020
Purpose: To build Frontend java project

=========================================
*/

pipeline {

	agent {

		label 'master'
	}
	tools {

		 maven 'mymaven'  // 
		 jdk 'java11'
	}

	stages{
	
		stage ('Code Checkout'){
			steps {
			git credentialsId: 'devops_git', url: 'https://github.com/Alsamad-GH/addressbook.git'
			}
		}

		stage('Validate project') {
		steps {

			script{
				sh '''
					mvn validate
				   '''
				   
				echo "this is Validate Project "
			}
		}
	}

	
	

		stage('Compile project') {
		steps {

			script{
				sh '''
					mvn compile
				   '''
			}
		}
	}

}
}
